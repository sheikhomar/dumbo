Program

	number result
	number fahrenheittemp
	
	result := fahrenheitTemperature(0.0, fahrenheitTemp)
	write("Freezing point: " + convertnumbertotext(result) + " F.")
	
	result := fahrenheitTemperature(100.0, fahrenheitTemp)
	write("Boiling point: " + convertnumbertotext(result) + " F.")
	
End program

Function FahrenheitTemperature(number celciusTemp, number fahrenheitTemp) return number
	return (9.0 / 5.0) * celciusTemp + 32.0
end function