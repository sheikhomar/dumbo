Program
	Number i

	Repeat (100)
		Write("Fib(" + i + ") = " + fib(i))
	End Repeat
End Program


Function Fib(Number n) returns number
	Number result
	Table memo := 1x100 					// Not yet supported in syntax
	
	if(n = 0) then
		result = 0
	else if(n = 1) then
		result = 1
	else if NOT(memo[n] = 0) then
		result = memo[n]
	else
		result = fib(n-1) + fib(n-2)
		memo[n] = result
	End if

	return result
End function
