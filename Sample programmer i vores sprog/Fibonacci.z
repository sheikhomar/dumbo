Method Fib(Number n)
	Number result
	Table memo := 1x100
	
	if(n = 0) then
		result = 0
	end if
	else if(n = 1) then
		result = 1
	End if
	else if(memo[n] != 0) then
		result = memo[n]
	End if
	else then
		result = fib(n-1) + fib(n-2)
		memo[n] = result
	End if

	return result
End Method

Program
	Number i

	Repeat 100
		Write("Fib(") Write(i) Write(")") = Write(fib(i))
	End Repeat
End Program
