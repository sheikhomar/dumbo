Program
	Write("Enter three numbers seperated by enter")
	Number inputNumb1
	Number inputNumb2
	Number inputNumb3
	inputNumb1 := ReadNumber()
	inputNumb2 := ReadNumber()
	inputNumb3 := ReadNumber()
	write("" + Multiplier(inputNumb1))
	
	//write("" + Circle(inputNumb1, inputNumb2, inputNumb3))
	
End Program

Function Multiplier(number inputNumb1) return number
	return inputNumb1 * inputNumb1
End Function 

Function Circle(number inputNumb1, number inputNumb2, number inputNumb3) returns boolean
	if(inputNumb1*inputNumb1 + inputNumb2*inputNumb2 <= inputNumb3*inputNumb3) then
		write("The point(" + inputNumb1 + "," + inputNumb2 + _
		") is inside the circle centered in (0,0) and a radius " + inputNumb3)
	else 
		write("The point(" + inputNumb1 + "," + inputNumb2 + _
		") is outside the circle centered in (0,0) and a radius " + inputNumb3)
	end if
	
	return true
End Function 
